{
  "name": "@proofdict/tester-cli",
  "version": "3.0.0",
  "description": "A CLI for @proofdict/tester.",
  "keywords": [
    "cli",
    "proofdict"
  ],
  "homepage": "https://github.com/proofdict/proofdict/tree/master/packages/@proofdict/tester-cli/",
  "bugs": {
    "url": "https://github.com/proofdict/proofdict/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "bin": {
    "proofdict-tester": "./bin/cmd.js"
  },
  "main": "lib/tester-cli.js",
  "types": "lib/tester-cli.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proofdict/proofdict.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib/"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "cross-env": "^7.0.2",
    "mocha": "^8.1.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.9.7"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4
  },
  "dependencies": {
    "@proofdict/tester": "^3.0.0",
    "@proofdict/types": "^3.0.0",
    "@types/globby": "^9.1.0",
    "@types/js-yaml": "^3.12.1",
    "@types/meow": "^5.0.0",
    "globby": "^10.0.1",
    "js-yaml": "^3.13.1",
    "meow": "^5.0.0"
  }
}
